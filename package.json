{
  "name": "logicstamp-context",
  "version": "0.1.0",
  "description": "Generate AI-friendly context bundles from React codebases with LogicStamp contracts",
  "type": "module",
  "main": "./dist/index.js",
  "bin": {
    "logicstamp-context": "./dist/cli/index.js",
    "logicstamp-validate": "./dist/cli/validate-index.js"
  },
  "scripts": {
    "build": "tsc",
    "prepublishOnly": "npm run build",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage"
  },
  "keywords": [
    "react",
    "typescript",
    "ai",
    "llm",
    "context",
    "documentation",
    "logicstamp",
    "claude",
    "chatgpt",
    "codebase-analysis",
    "ast",
    "static-analysis"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/logicstamp-context.git"
  },
  "bugs": {
    "url": "https://github.com/yourusername/logicstamp-context/issues"
  },
  "homepage": "https://github.com/yourusername/logicstamp-context#readme",
  "author": "LogicStamp Contributors",
  "license": "MIT",
  "dependencies": {
    "glob": "^10.3.10",
    "ts-morph": "^21.0.1"
  },
  "devDependencies": {
    "@types/node": "^20.11.5",
    "@vitest/ui": "^4.0.8",
    "typescript": "^5.3.3",
    "vitest": "^4.0.8"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
